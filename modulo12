library(MASS)
install.packages("vegan")
library(vegan)
data(dune)
View(dune)

ir.pca <- prcomp(dune,
                 center = TRUE,
                 scale. = TRUE) 
print(ir.pca)
plot(ir.pca, type = "l")
summary(ir.pca)
predict(ir.pca, 
        newdata=tail(dune, 2))
library(devtools)
install_github("vqv/ggbiplot")
library(ggbiplot)
g <- ggbiplot(ir.pca, obs.scale = 1, var.scale = 1, ellipse = TRUE, 
              circle = TRUE)
g <- g + scale_color_discrete(name = '')
g <- g + theme(legend.direction = 'horizontal', 
               legend.position = 'top')
print(g)

